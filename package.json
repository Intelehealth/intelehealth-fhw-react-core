{
  "name": "@intelehealth/intelehealth-fhw-react-core",
  "version": "1.0.0",
  "description": "Intelehealth FHW React Core",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./core": {
      "import": "./dist/core/index.esm.js",
      "require": "./dist/core/index.js",
      "types": "./dist/core/index.d.ts"
    },
    "./services": {
      "import": "./dist/services/index.esm.js",
      "require": "./dist/services/index.js",
      "types": "./dist/services/index.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks/index.esm.js",
      "require": "./dist/hooks/index.js",
      "types": "./dist/hooks/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.esm.js",
      "require": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.esm.js",
      "require": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "yarn type-check && yarn lint:check && rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "lint:check": "eslint src/**/*.{ts,tsx} --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "docs": "typedoc --out docs src/index.ts",
    "clean": "rimraf dist",
    "prepare": "husky install",
    "pre-commit": "lint-staged && yarn format && yarn type-check && yarn lint:check && yarn test",
    "pre-push": "yarn type-check && yarn lint:check && yarn test && yarn build",
    "prepublishOnly": "yarn clean && yarn build",
    "publish": "npm publish",
    "prebuild": "yarn clean",
    "postbuild": "echo 'Build completed'",
    "link:dev": "node scripts/link-dev.js",
    "link:dev:win": "scripts\\link-dev.bat",
    "link:dev:unix": "chmod +x scripts/link-dev.sh && scripts/link-dev.sh"
  },
  "keywords": [
    "react",
    "react-native",
    "intelehealth",
    "healthcare",
    "universal",
    "library",
    "typescript",
    "core"
  ],
  "author": "Intelehealth <info@intelehealth.org>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/intelehealth/intelehealth-fhw-react-core.git"
  },
  "bugs": {
    "url": "https://github.com/intelehealth/intelehealth-fhw-react-core/issues"
  },
  "homepage": "https://github.com/intelehealth/intelehealth-fhw-react-core#readme",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.6.8"
  },
  "devDependencies": {
    "tslib": "^2.8.1",
    "@eslint/js": "^8.57.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "ts-jest": "^29.0.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
